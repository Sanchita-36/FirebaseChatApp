# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'FirebaseChatApp' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for FirebaseChatApp
	pod 'Firebase/Core' 
    	pod 'Firebase/Auth'
    	pod 'Firebase/Database'

  # Pods for Facebook Login
    	pod 'FBSDKLoginKit'

  # Pods for Google Login
    	pod 'GoogleSignIn', '4.4.0'

	pod 'MessageKit'
        pod 'JGProgressHUD'
	pod 'SDWebImage'
        # pod 'RealmSwift'
	
end
